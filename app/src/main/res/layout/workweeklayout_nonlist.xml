                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:text="@string/active"
                android:enabled="true"
                android:layout_gravity="end">

            </CheckBox>
        </GridLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/we_layout"
        android:clickable="true"
        android:layout_width="match_parent"
        android:elevation="4dp"
        android:paddingTop="4dp"
        android:background="@drawable/border_stroke"
        android:backgroundTint="@android:color/white"
        android:paddingBottom="4dp"
        android:orientation="vertical"
        android:layout_marginBottom="@dimen/list_item_margin"
        android:layout_height="wrap_content">
        <TextView

            android:id="@+id/we_day_of_week"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:paddingStart="4dp"
            android:paddingEnd="4dp"
            android:paddingBottom="2dp"
            android:text="@string/Wednesday"/>

        <GridLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView

                android:id="@+id/we_trips_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:paddingLeft="4dp"
                android:paddingStart="4dp"
                android:paddingRight="4dp"
                android:paddingEnd="4dp"
                android:text="Start Time"/>

            <CheckBox
                android:id="@+id/we_workday_list_item_activeCheckBox"
                android:button="@null"
                android:drawableEnd="?android:attr/listChoiceIndicatorMultiple"
                android:theme="@style/CommuteCheckBox"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:text="@string/active"
                android:enabled="true"
                android:layout_gravity="end">

            </CheckBox>
        </GridLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/th_layout"
        android:clickable="true"
        android:layout_width="match_parent"
        android:elevation="4dp"
        android:paddingTop="4dp"
        android:paddingBottom="4dp"
        android:orientation="vertical"
        android:layout_marginBottom="@dimen/list_item_margin"
        android:background="@drawable/border_stroke"
        android:backgroundTint="@android:color/white"
        android:layout_height="wrap_content">
        <TextView

            android:id="@+id/th_day_of_week"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
